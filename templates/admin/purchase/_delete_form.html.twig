<form method="post" action="{{ path('admin_purchase_delete', {'id': purchase.id}) }}" onsubmit="return confirm('Etes vous sur de vouloir supprimer cette commande ?');">
	<input type="hidden" name="_method" value="DELETE">
	<input type="hidden" name="_token" value="{{ csrf_token('delete' ~ purchase.id) }}">
	<button class="btn">Supprimer</button>
</form>
